        "font-awesome-scss": "^1.0.0",
        "jquery": "^3.1.1",
        "laravel-mix": "^1.0",
        "locutus": "^2.0.9",
        "lodash": "^4.17.4",
        "moment": "^2.22.2",
        "sass-loader": "^6.0.5",
        "vue": "^2.5.7",
        "vue-strap": "github:wffranco/vue-strap",
        "webpack-notifier": "^1.15.0"
    },
    "dependencies": {
        "ajv": "^4.9.1",
        "bootstrap-sass": "^3.3.7",
        "vue-router": "^3.6.5",
        "webpack": "^3.5.0"
    }
